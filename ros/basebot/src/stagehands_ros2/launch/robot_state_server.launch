<launch>
    <arg name="multi_robot_name" default=""/>
    <arg name="set_lidar_frame_id" default="base_scan"/>
    <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
    <node pkg="stagehands_ros2" type="run_robot_state_server.py" name="stagehands_ros2_state_server" output="screen" launch-prefix="sudo -E PYTHONPATH=$(env PYTHONPATH)"/>
</launch>